<?xml version="1.0"?>
<!DOCTYPE CodeBlocks_compiler>
<CodeBlocks_compiler name="GNU GCC MinGW Compiler"
                     id="gcc-mingw32"
                     weight="60"
                     platform="windows">
    <Path type="master">
        <Search path="C:\mingw"
            for="C"/>
        <Search path="C:\mingw\mingw32"
            for="C"/>
        <Search path="C:\mingw-32"
            for="C"/>
        <Search path="C:\mingw-32\mingw32"
            for="C"/>
        <Search path="C:\mingw-64\mingw32"
            for="C"/>
        <Search macro="$(CODEBLOCKS)\MinGW32"
            for="C"/>
        <Search macro="$(CODEBLOCKS)\MinGW"
            for="C"/>
        <Fallback path="C:\mingw"/>
    </Path>
    <Path type="include">
        <Add><master/>\include</Add>
    </Path>
    <Path type="lib">
        <Add><master/>\lib</Add>
    </Path>
</CodeBlocks_compiler>
